            '1L': {name: 'Disney\'s Port Orleans Resort - Riverside', category: 'moderate'},
            '1Y': {name: 'Disney\'s Yacht Club Resort', category: 'deluxe'},
            '1FW': {name: 'The Cabins at Disney\'s Fort Wilderness Resort', category: 'moderate'},
            '1F': {name: 'The Campsites at Disney\'s Fort Wilderness Resort', category: 'campground'},
            '1X': {name: 'The Villas at Disney\'s Wilderness Lodge', category: 'deluxe-villa'},
            SWN: {name: 'Walt Disney World Swan Hotel', category: 'other-select-deluxe-hotels'},
            DOL: {name: 'Walt Disney World Dolphin Hotel', category: 'other-select-deluxe-hotels'},
            '1J': {name: 'Disney\'s Wilderness Lodge', category: 'deluxe'},
            '1P': {name: 'Disney\'s Polynesian Village Resort', category: 'deluxe'}
        },
        tagIdMap = obj(
            SEARCH, 27173,
            CART, 28655,
            CONVERSION, 28657,
            VISITOR, 28659
        ),
        lastTagType = VISITOR;

    Handlers.push(
        {events: 'parseModelComplete.framework', handler: process},
        {events: 'siteCatalystCallComplete', handler: wishlistAdd}
    );

    waitFor({
        location: 'lpTag.events.bind',
        callback: function (bind) {
            // Interesting LP Events: LP_INV_SHOWN, LP_INV_ACCEPT, LP_INV_DECLINE, LP_INV_TIMEOUT, LP_DYNBUTTON_CLICKED
            bind({
                eventName: 'LP_DYNBUTTON_CLICKED',
                func: function () {
                    fireBKTag({eventType: 'chat', eventSubType: getPageType() === CART ? 'inCart' : 'outCart'});
                }
            });
        },
        failCallback: noop,
        timeout: 45000,
        interval: 1000
    });

    if (location.pathname.match(/^\/dining\//)) {
        waitFor({
            location: [
                'WDPRO.Analytics.wdwQuery',
                function ($) {
                    return $('#diningReservationFormContainer #searchButton')[0];
                }
            ],
            callback: function (wdwQuery, searchButton) {
                $ = wdwQuery;
                $(searchButton).on('click', function () {
                    var facetStr = location.hash.replace(/^#\/|\/$/g, '');
                    if (facetStr) {
                        fireBKTag({facet: facetStr.replace(/,/g, '|')}, SEARCH);
                    }
                });
            },
            failCallback: noop,
            timeout: 40000,
            interval: 500
        });
    }

    function wishlistAdd(e, data) {
        var wishlistType;
        if (data.trackingType === 'trackClick' &amp;&amp; strContains(ref('dataSent.prop9', data), 'WishlistAdd')) {
            if (strContains(data.dataSent.prop9, '_Attraction_')) {
                wishlistType = 'attraction';
            } else if (strContains(data.dataSent.prop9, '_resort_')) {
                wishlistType = 'hotel';
            } else if (strContains(data.dataSent.prop9, '_restaurant_')) {
                wishlistType = 'dining';
            }
            if (wishlistType) {
                fireBKTag({eventType: 'wishlist', eventSubType: wishlistType});
            }
        }
    }

    function process(e, sam) {
        if (!ref('configuration.SiteCatalyst', sam)) return;
        var pageType = getPageType(sam),
            pageMap = obj(
                SEARCH, searchTag,
                CART, cartTag,
                CONVERSION, conversionTag,
                VISITOR, visitorTag
            );
        $ = ref('WDPRO.Analytics.wdwQuery');
        addCommonVars(vars, sam);
        pageMap[pageType](vars, sam);
    }

    function getSAM() {
        if ((F = A.Framework) &amp;&amp; toType(F.analyticsModel) === 'array' &amp;&amp; F.analyticsModel.length &gt; 0) {
            return F.analyticsModel[0];
        }
    }

    function addCommonVars(vars, sam) {
        _set(vars, {
            id: getSWID(),
            pageKey: $('meta[name=pageKey]').attr('content')
        });
        return vars;
    }

    function getPageType(sam) {
        sam = sam || getSAM() || {};
        if (
            strContains(sam.events, 'purchase') ||
            sam.pageId === 'confirmsuccess'
        ) {
            return CONVERSION;
        } else if (
            strContains(sam.siteSection, '/cart/') ||
            strContains(sam.events, 'cart') ||
            ref('carts.0.id', sam) ||
            strContains(ref('products.0.events', sam), 'cart')
        ) {
            return CART;
        } else if (
            vpg.enabled &amp;&amp;
            strContains(sam.siteSection, vpg.siteSectionIndicator)
        ) {
            return VISITOR;
        } else if (
            sam.bookingSearchArrivalDate
        ) {
            return SEARCH;
        }
        return VISITOR;
    }

    function getSWID() {
        return ref('PEP.guestLocators.swid') ||
            ref('WDPRO.Analytics.Framework.analyticsModel.0.customerId') ||
            cookieGet('SWID') ||
            cookieGetObject('stateManager_jar', 'swid') ||
            cookieGetObject('rememberme', 'swid');
    }

    function getFinderFacets(sam) {
        if (ref('PEP.Finder')) {
            return ((location.hash || '').match(/^#\/(.+?)\/?$/) || [])[1];
        }
    }

    function splitCategoryFacets(facetStr) {
        var facets = {},
            facetArr,
            propName;

        if (facetStr) {
            facetArr = facetStr.split(',');
            facetArr.forEach(function (facet) {
                propName = resortCategories.indexOf(facet) &gt; -1 ? 'category' : 'other';
                (facets[propName] = facets[propName] || []).push(facet);
            });
            if (facets.category) {
                facets.category = facets.category.join(',');
            }
            if (facets.other) {
                facets.other = facets.other.join(',');
            }
        }
        return facets;
    }

    function dateFormat(date, fmt) {
        var moment = ref('WDPRO.Analytics.moment');
        if (date &amp;&amp; moment &amp;&amp; (date = moment(date)).isValid()) {
            return date.format(fmt);
        }
    }

    function vpgCategory(linkCategory) {
        var transformMap = {
            PARK_ACTIVITY: 'ParkActivity',
            RESORT_FEATURES: 'ResortFeatures',
            DINING_EXPERIENCES: 'DiningExperiences'
        };
        return transformMap[linkCategory];
    }

    function vpgValue(selection) {
        var transformMap = {
            'wdpr-wdw-o-vpg-q-style-generations': 'Generations',
            'wdpr-wdw-o-vpg-q-style-group': 'BigGroup',
            'wdpr-wdw-o-vpg-q-style-family': 'MyFamily',
            'wdpr-wdw-o-vpg-q-style-active': 'Active',
            'wdpr-wdw-o-vpg-q-style-splurge': 'Splurge',
            'wdpr-wdw-o-vpg-q-style-romantic': 'Romantic',
            'wdpr-wdw-o-vpg-q-style-spoccasion': 'SpecialOccasion',
            'wdpr-wdw-o-vpg-q-style-budget': 'Budget',
            'wdpr-wdw-o-vpg-q-style-relaxed': 'Relaxed',

            'wdpr-wdw-o-vpg-q-pref-activities-princess': 'DisneyPrincesses',
            'wdpr-wdw-o-vpg-q-pref-activities-paradesfireworks': 'ParadesampFireworks',
            'wdpr-wdw-o-vpg-q-pref-activities-thrill': 'ThrillRides',
            'wdpr-wdw-o-vpg-q-pref-activities-space': 'SpaceAdventures',
            'wdpr-wdw-o-vpg-q-pref-activities-classics': 'DisneyParkClassics',
            'wdpr-wdw-o-vpg-q-pref-activities-newatparks': 'NewattheParks',
            'wdpr-wdw-o-vpg-q-pref-activities-scary': 'ScaryGoodFun',
            'wdpr-wdw-o-vpg-q-pref-activities-animal': 'AnimalAttractions',
            'wdpr-wdw-o-vpg-q-pref-activities-pirate': 'PiratesAhoy',
            'wdpr-wdw-o-vpg-q-pref-activities-water': 'WaterParksampRides',
            'wdpr-wdw-o-vpg-q-pref-activities-showsmusic': 'LiveShowsampMusic',
            'wdpr-wdw-o-vpg-q-pref-activities-disneyfan': 'ForDisneyFanatics',
            'wdpr-wdw-o-vpg-q-pref-activities-characters': 'CharacterExperiences',

            'wdpr-wdw-o-vpg-q-pref-resort-pools': 'WayCoolPools',
            'wdpr-wdw-o-vpg-q-pref-resort-storyexp': 'StoryExperienceRooms',
            'wdpr-wdw-o-vpg-q-pref-resort-waterfront': 'WaterfrontResorts',
            'wdpr-wdw-o-vpg-q-pref-resort-golf': 'Golf',
            'wdpr-wdw-o-vpg-q-pref-resort-accessible': 'AccessibleRooms',
            'wdpr-wdw-o-vpg-q-pref-resort-kitchenette': 'Kitchenettes',
            'wdpr-wdw-o-vpg-q-pref-resort-fitness': 'FitnessCenter',
            'wdpr-wdw-o-vpg-q-pref-resort-views': 'IncredibleViews',
            'wdpr-wdw-o-vpg-q-pref-resort-spasalon': 'SpaampSalon',

            'wdpr-wdw-o-vpg-q-pref-dining-asian': 'AsianCuisine',
            'wdpr-wdw-o-vpg-q-pref-dining-fireworks': 'FireworksDining',
            'wdpr-wdw-o-vpg-q-pref-dining-latenight': 'LateNightEats',
            'wdpr-wdw-o-vpg-q-pref-dining-italian': 'ItalianCuisine',
            'wdpr-wdw-o-vpg-q-pref-dining-character': 'CharacterDining',
            'wdpr-wdw-o-vpg-q-pref-dining-foodie': 'FoodieFavorite',
            'wdpr-wdw-o-vpg-q-pref-dining-wine': 'GreatWineSelection',
            'wdpr-wdw-o-vpg-q-pref-dining-special': 'SpecialExperiences',
