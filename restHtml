        return transformMap[selection] || selection;
    }

    function vpgSeasonMonths(selection) {
        var monthsMap = {
            '01': 'Jan',
            '02': 'Feb',
            '03': 'Mar',
            '04': 'Apr',
            '05': 'May',
            '06': 'Jun',
            '07': 'Jul',
            '08': 'Aug',
            '09': 'Sep',
            '10': 'Oct',
            '11': 'Nov',
            '12': 'Dec',
            winter: 'Jan,Feb',
            spring: 'Mar,Apr,May',
            summer: 'Jun,Jul,Aug',
            autumn: 'Sep,Oct,Nov',
            holidays: 'Nov,Dec',
            'any-season': 'Jan,Feb,Mar,Apr,May,Jun,Jul,Aug,Sep,Oct,Nov,Dec'
        };
        return monthsMap[selection] || [];
    }

    function vpgData(pageId) {
        var result = {},
            category,
            data,
            val,
            val2,
            frm;

        // angular.element('[ng-app]').injector().get('guestSelections').data
        if (
            (data =     window.angular         ) &amp;&amp; typeof data.element === 'function' &amp;&amp;
            (data = data.element('[ng-app]')   ) &amp;&amp; typeof data.injector === 'function' &amp;&amp;
            (data = data.injector()            ) &amp;&amp; typeof data.get === 'function' &amp;&amp;
            (data = data.get('guestSelections')) &amp;&amp;
            (data = data.data                  )
        ) {
            switch (pageId) {
                case 'recommendation':
                    if (typeof data.travelTimes === 'object' &amp;&amp; 'useFlexibleNotExact' in data.travelTimes) {
                        if (data.travelTimes.useFlexibleNotExact) {
                            if ((val = vpgSeasonMonths(ref('flexible.seasonsId', data.travelTimes)))) result.TravelMonth = val;
                        } else {
                            if (
                                (val = ref('exact.checkInDate', data.travelTimes)) &amp;&amp;
                                (val = vpgSeasonMonths(val.substr(5, 2))) &amp;&amp;
                                (val2 = ref('exact.checkOutDate', data.travelTimes)) &amp;&amp;
                                (val2 = vpgSeasonMonths(val2.substr(5, 2)))
                            ) {
                                result.TravelMonth = val + (val2 === val ? '' : ',' + val2);
                            }
                        }
                    }
                    result.bookingSearchAdults = ref('partyMix.adultCount', data);
                    result.bookingSearchKids = ref('partyMix.kidCount', data);
                    if (toType((val = ref('partyMix.nonAdults', data))) === 'array' &amp;&amp; val.length &gt; 0) {
                        result.bookingSearchChildrenAges = val.join(',');
                        result.youngestChild = val.reduce(function (p, c) {
                            return p &lt; c ? p : c;
                        });
                    }

                    if ((frm = document.getElementById('addToCartForm'))) {
                        frm.submit = function () {
                            fireBKTag(_set(vars, 'eventType', 'AddRecommendationToCart'));
                            setTimeout(function () {
                                HTMLFormElement.prototype.submit.call(frm);
                            }, 580);
                        };
                    }

                case 'traveldetails':
                    if (toType(data.vacationChoices) === 'array' &amp;&amp; data.vacationChoices.length &gt; 0) {
                        data.vacationChoices.forEach(function (choice) {
                            if ((category = vpgCategory(choice.type))) {
                                result[category] = result[category] || [];
                                result[category].push(vpgValue(choice.id));
                            }
                        });
                        ['ParkActivity','ResortFeatures','DiningExperiences'].forEach(function (category) {
                            if (result[category]) result[category] = result[category].join(',');
                        });
                    }

                case 'travelpreferences':
                    if (toType(data.travelStyles) === 'array' &amp;&amp; data.travelStyles.length &gt; 0) {
                        result.TravelStyle = data.travelStyles.map(vpgValue).join(',');
                    }

                case 'travelstyle':
                case 'landing':

            }
        }

        return result;
    }

    function setP13nData(vars, sam) {
        var data = {
            id: '',
            locations: [],
            contentIds: [],
            customerGroups: [],
            vpcSegment: ''
        };

        if (sam.personalizations) {
            var p = sam.personalizations;
            sam.personalizations.forEach(function(el) {
                var locationType = el.locationType || 'standard';
                if (locationType !== 'data') {
                    data.locations.push(el.location);
                    data.contentIds.push(getContentIdFromLocation(el));
                    data.customerGroups.push(el.customerGroup);
                }
                if (el.vpcSegment) {
                    data.vpcSegment = el.vpcSegment;
                }
            });
        }
        if (WDPRO.Analytics.getPersonalizationId) {
            data.id = WDPRO.Analytics.getPersonalizationId();
            if (data.locations.length &gt; 0) {
                _set(vars, 
                    {
                        p13nLocations: data.locations.join(','),
                        p13nContentIds: data.contentIds.join(','),
                        p13nCustomerGroups: data.customerGroups.join(','),
                        p13nVpcSegment: data.vpcSegment || '',
                        p13nId: data.id || ''
                    }
                );
            }
        }
    }

    function getContentIdFromLocation(location) {
        var contentId = '';
        if (location &amp;&amp; location.options) {
            location.options.forEach(function (el) {
                if (el.decision) {
                    contentId = el.decision;
                }
            })
        } else if (location &amp;&amp; location.customerGroup &amp;&amp; /^(?:c|c1|c2)$/.test(location.customerGroup)) {
            contentId = 'DEFAULT'
        }
        return contentId;
    }

    function getResort(pkg, product, sam) {
        var facilityIds = [],
            resort;

        if (pkg &amp;&amp; pkg.facilityId) {
            return resorts[pkg.facilityId];
        }

        if (product &amp;&amp; product.facilityId) {
            return resorts[product.facilityId];
        }

        if (toType(sam.productsDisplayed) === 'array') {
            sam.productsDisplayed.forEach(function (product) {
                if (product.facilityId) {
                    facilityIds.push(product.facilityId);
                }
            });
        }
        if (facilityIds.length &gt; 0) {
            resort = resorts[facilityIds[0]];
            facilityIds.forEach(function (id, i, arr) {
                if (id !== arr[0]) {
                    resort = undefined;
                }
            });
        }
        return resort;
    }

    function getVacationPackage(sam) {
        var package;
        if (toType(sam.products) === 'array') {
            sam.products.forEach(function (product) {
                if (
                    !package &amp;&amp;
                    product.productType === 'resortVacation' &amp;&amp;
                    !strContains(product.events, 'cartRemove')
                ) {
                    package = product;
                }
            });
        }
        return package;
    }

    function getBookingSearch(type, data) {
        var response,
            paths = [
                'pkg.booked' + type,
                'pkg.bookingSearch' + type,
                'sam.bookingSearch' + type,
                'sam.totalBooked' + type
            ];
        paths.forEach(function (path) {
            if (response === undefined) {
                response = ref(path, data);
            }
        });
        return response;
    }

    function visitorTag(vars, sam) {
        var facets = splitCategoryFacets(getFinderFacets(sam));

        _set(vars, {
            contentLang: ref('configuration.contentLanguage', sam),
            contentLocale: ref('configuration.contentLocale', sam),
            SiteSection: sam.siteSection,
            pageID: sam.pageId,
            facet: facets.other,
            FacilityCategories: facets.category,
        });

        if (vpg.enabled &amp;&amp; sam.siteSection === vpg.siteSectionIndicator) {
            _set(vars, vpgData(sam.pageId));
        }

        setP13nData(vars, sam);

        fireBKTag(vars, VISITOR);
    }

    function searchTag(vars, sam) {
        var facets = splitCategoryFacets(getFinderFacets(sam)),
            pkg = getVacationPackage(sam),
            product = ref('products.0', sam),
            resort = getResort(pkg, product, sam),
            data = {resort: resort, product: product, pkg: pkg, sam: sam};

        _set(vars, {
            bookingSearchAdults: +getBookingSearch('Adults', data),
            bookingSearchKids: +getBookingSearch('Children', data) + +getBookingSearch('Infants', data),
            bookingSearchChildrenAges: (getBookingSearch('ChildrenAges', data) || []).join(',') || undefined,
            ProductID: ref('pkg.productId', data) || ref('product.productId', data),
            ProductType: ref('pkg.productType', data) || ref('product.productType', data),
            bookingSearchArrivalDate: dateFormat(getBookingSearch('ArrivalDate', data), 'YYYY-MM-DD'),
            bookingSearchLengthOfStayNights: +getBookingSearch('LengthOfStayNights', data),
            FacililtyName: ref('resort.name', data),
            FacilityCategories: ref('resort.category', data) || facets.category,
            packageType: ref('pkg.packageType', data),
            facet: facets.other
        });

        setP13nData(vars, sam);

        fireBKTag(vars, SEARCH);
    }

    function cartTag(vars, sam) {
        var facets = splitCategoryFacets(getFinderFacets(sam)),
            pkg = getVacationPackage(sam),
            product = ref('products.0', sam),
            resort = getResort(pkg, product, sam),
            data = {resort: resort, product: product, pkg: pkg, sam: sam};

        _set(vars, {
            ProductID: multiRef('productId', sam.products, '/'), //ref('pkg.productId', data) || ref('product.productId', data),
            ProductType: multiRef('productType', sam.products, '/'), //ref('pkg.productType', data) || ref('product.productType', data),
            days: multiRef('days', sam.products, '/'), //ref('product.days', data),
            name: multiRef('name', sam.products, '/'), //ref('product.name', data),
            parks: multiRef('venues', sam.products, '/'),
            FacililtyName: ref('resort.name', data),
            FacilityCategories: ref('resort.category', data) || facets.category,
            packageType: ref('pkg.packageType', data),
            facet: facets.other,
            bookingSearchAdults: +getBookingSearch('Adults', data),
            bookingSearchKids: +getBookingSearch('Children', data) + +getBookingSearch('Infants', data),
            bookingSearchChildrenAges: (getBookingSearch('ChildrenAges', data) || []).join(',') || undefined,
            bookingSearchArrivalDate: dateFormat(getBookingSearch('ArrivalDate', data), 'YYYY-MM-DD'),
            bookingSearchLengthOfStayNights: +getBookingSearch('LengthOfStayNights', data)
        });

        setP13nData(vars, sam);

        fireBKTag(vars, CART);
    }

    function conversionTag(vars, sam) {
        var facets = splitCategoryFacets(getFinderFacets(sam)),
            pkg = getVacationPackage(sam),
            product = ref('products.0', sam),
            resort = getResort(pkg, product, sam),
            data = {resort: resort, product: product, pkg: pkg, sam: sam};

        _set(vars, {
            ProductID: multiRef('productId', sam.products, '/'), //ref('pkg.productId', data) || ref('product.productId', data),
            ProductType: multiRef('productType', sam.products, '/'), //ref('pkg.productType', data) || ref('product.productType', data),
            days: multiRef('days', sam.products, '/'), //ref('product.days', data),
            name: multiRef('name', sam.products, '/'), //ref('product.name', data),
            parks: multiRef('venues', sam.products, '/'),
            FacililtyName: ref('resort.name', data),
            FacilityCategories: ref('resort.category', data) || facets.category,
            packageType: ref('pkg.packageType', data),
            facet: facets.other,
            bookingSearchAdults: +getBookingSearch('Adults', data),
            bookingSearchKids: +getBookingSearch('Children', data) + +getBookingSearch('Infants', data),
            bookingSearchChildrenAges: (getBookingSearch('ChildrenAges', data) || []).join(',') || undefined,
            bookingSearchArrivalDate: dateFormat(getBookingSearch('ArrivalDate', data), 'YYYY-MM-DD'),
            bookingSearchLengthOfStayNights: +getBookingSearch('LengthOfStayNights', data)
        });

        setP13nData(vars, sam);

        fireBKTag(vars, CONVERSION);
    }

    function fireBKTag(vars, tagType) {
        objForEach(vars, function (val, key) {
            bk_addPageCtx(key, val);
        });
        bk_allow_multiple_calls = true;
        bk_use_multiple_iframes = true;
        bk_doJSTag(tagIdMap[(lastTagType = tagType || lastTagType)], 10);
    }

    function toType(o) {
        return Object.prototype.toString.call(o).slice(8,-1).toLowerCase();
    }
    
    function multiRef(path, base, enclose) {
        var value = [];

        if (Array.isArray(base) &amp;&amp; base.length &gt; 0) {
            base.forEach(function(val) {
                value.push((enclose ? enclose : '') + ref(path, val) + (enclose ? enclose : ''));
            })
        }
        return value.length &gt; 0 ? value.join(',') : undefined;
    }

    function ref(path, base) {
        var i = path.indexOf('.');
        base = base || this;
        return (i === -1) ? base[path] : (base = base[path.substr(0, i)]) &amp;&amp; ref(path.substr(i + 1), base);
    }

    function strContains(str, key) {
        if (typeof str === 'string') {
            return str.indexOf(key) &gt; -1;
        }
    }

    function cookieGetObject(name, key) {
        var obj = _parse(cookieGet(name));
        if (key) {
            if (toType(obj) === 'object') {
                obj = ref(key, obj);
            } else {
                obj = undefined;
            }
        }
        return obj;
    }

    function cookieGet(key) {
        return cookiesGetAll()[key];
    }

    function cookiesGetAll() {
        var newCookieStr = document.cookie,
            cookieArr,
            i,
            cookie,
            index,
            cookieName;

        if (newCookieStr !== cookieString) {
            cookieString = newCookieStr;
            cookies = {};

            cookieArr = cookieString.split('; ');
            for (i = 0; i &lt; cookieArr.length; i++) {
                cookie = cookieArr[i];
                index = cookie.indexOf('=');
                if (index &gt; 0) {
                    cookieName = _decode(cookie.substring(0, index));
                    if (!(cookieName in cookies)) {
                        cookies[cookieName] = _decode(cookie.substring(index + 1));
                    }
                }
            }
        }
        return cookies;
    }

    function _decode(str) {
        try {
            str = decodeURIComponent(str);
        } catch (ignore) {
            // str is not decodable using decodeURIComponent
        }
        return str;
    }

    function _parse(str) {
        try {
            str = JSON.parse(str);
        } catch (ignore) {
            // str is not parsable
        }
        return str;
    }

    function isNotNaN(val) {
        return val === val;
    }

    function objForEach(r,n,o){var t;for(t in r)r.hasOwnProperty(t)&amp;&amp;n.call(o,r[t],t,r)} // jshint ignore:line

    function obj(){for(var o={},a=arguments,i=0,n=a.length;n&gt;i;)o[a[i++]]=a[i++];return o} // jshint ignore:line

    function merge(destination, source) {
        objForEach(source, function (val, key) { destination[key] = val; });
        return destination;
    }

    function _set(targetObj, key, val) {
        var srcObj = key;
        if (typeof key === 'string') {
            (srcObj = {})[key] = val;
        }
        objForEach(srcObj, function (val, key) {
            if (typeof val !== 'undefined' &amp;&amp; isNotNaN(val)) {
                targetObj[key] = val;
            }
        });
        return targetObj;
    }

    function noop() {}

    function waitFor(config) {
        if (!config._initialAttempt) {
            config = waitForConfig(config);
        }
        config._attempt++;

        var success = true,
            refs = [];

        config.location.forEach(function (loc) {
            refs.push(success &amp;&amp; (success = (typeof loc === 'function') ? loc.apply(null, refs) : ref(loc, config.base)));
        });

        if (success) {
            config.callback.apply(null, refs);
        } else if (config._attempt &lt; config._maxAttempts) {
            setTimeout(function () { waitFor(config); }, config.interval);
        } else {
            config.failCallback(config);
        }

        function waitForConfig(obj) {
            var locationType,
                config = {
                    location: null,              // string reference to desired property on base object (e.g. 'WDPRO.Analytics.wdwQuery')
                    base: window,                // base object on which to search
                    callback: null,              // function to receive the property value once it's found
                    failCallback: defaultFailFn, // function to call if propery is not found before the timeout
                    timeout: 30000,              // how many milliseconds after which to give up and call the failCallback
                    interval: 250                // how often during the timeout period to keep looking
                };

            merge(config, obj);

            locationType = toType(config.location);
            if (locationType !== 'string' &amp;&amp; locationType !== 'array' &amp;&amp; locationType !== 'function') {
                throw new TypeError('waitFor: config.location is required and must be a string, a function, or an array');
            }
            if (toType(config.callback) !== 'function') {
                throw new TypeError('waitFor: config.callback is required and must be a function');
            }

            if (locationType !== 'array') {
                config.location = [config.location];
            }
            return merge(config, {
                _initialAttempt: new Date().getTime(),
                _attempt: 0,
                _maxAttempts: config.timeout / config.interval
            });
        }

        function defaultFailFn(config) {
            if (toType(ref('console.log')) === 'function') {
                console.log(
                    '"%s" not found after %dms (%d attempts, delaying %dms)',
                    config.location.join(','),
                    new Date().getTime() - config._initialAttempt,
                    config._attempt,
                    config.interval
                );
            }
        }
    }

}());
</script>
