	scriptSource = window.ClickTaleScriptSource,
	pccSource = scriptSource;

if (!scriptSource) {
	window.ClickTaleScriptSource = isHttps ? 'https://cdnssl.clicktale.net/www/' : 'http://cdn.clicktale.net/www/';
}

ClickTaleGlobal.init.monitorScriptName = "Monitor-latest.js";
ClickTaleGlobal.init.isAllowed &amp;&amp; typeof ClickTaleGlobal.init.pmc === "function" &amp;&amp; ClickTaleGlobal.init.pmc(autoMonitorConfig);

if(!ClickTaleGlobal.init.pccRequested) {
	var pccSrc = pccSource ? pccSource + '73e43e63-0ccd-4ba6-b9a7-86141e6b5e85.js?DeploymentConfigName=Release_20180903&amp;Version=2' : (isHttps ? 'https://cdnssl.clicktale.net/www49/pcc/73e43e63-0ccd-4ba6-b9a7-86141e6b5e85.js?DeploymentConfigName=Release_20180903&amp;Version=2' : 'http://cdn.clicktale.net/www49/pcc/73e43e63-0ccd-4ba6-b9a7-86141e6b5e85.js?DeploymentConfigName=Release_20180903&amp;Version=2');
		var pccScriptElement = ClickTaleCreateDOMElement('script');
	pccScriptElement.type = "text/javascript";
	pccScriptElement.crossOrigin = "anonymous";
	pccScriptElement.src = pccSrc;
	ClickTaleGlobal.init.isAllowed &amp;&amp; document.body.appendChild(pccScriptElement);
		ClickTaleGlobal.init.pccRequested = true;
}
	window.ClickTaleGlobal.PCCExists = true;
	
window.ClickTalePrevOnReady = typeof window.ClickTaleOnReady == 'function' ? window.ClickTaleOnReady : void 0;

window.ClickTaleOnReady = function() {
	var PID=6, 
		Ratio=0.5, 
		PartitionPrefix="www49",
		SubsId=233474;
	
	if (window.navigator &amp;&amp; window.navigator.loadPurpose === "preview") {
       return;
	};
		
	
	// Start of user-defined header code (PreInitialize)
	//Cross Domain Tracking Start
function clickTaleCDStartRecorder() {

    //Cross Domain Tracking Start
    if (window.ClickTaleSettings.PTC.CrossDomainUID) {
        ClickTaleSetUID(window.ClickTaleSettings.PTC.CrossDomainUID);
    };
    //Cross Domain Tracking End

    if (typeof ClickTaleSetAllSensitive === "function") {
        ClickTaleSetAllSensitive();
    }
    if (/(?:disneyland.disney.go.com|dlr-stage.disney.go.com|dlr-latest.disney.go.com)/i.test(document.location.hostname.toLowerCase())) {
        var aHotelEl;
        window.ClickTaleSettings.PTC.doOnlyWhen(function () {
            var i;
            for (i = 0; i &lt; aHotelEl.length; i++) {
                window['ClickTaleSetCustomElementID'](aHotelEl[i], 'hotelCard' + i);
            }
        }, function () {
            aHotelEl = document.querySelectorAll('.detailPageLink a');
            return (typeof window['ClickTaleSetCustomElementID'] === 'function' &amp;&amp; aHotelEl.length)
        }, 400, 30);

    }

    window.ClickTaleSettings.PTC.InitFuncs = window.ClickTaleSettings.PTC.InitFuncs || [];
    window.ClickTaleSettings.PTC.InitFuncs.push(function () {
        var pcc = document.querySelector('script[src*="clicktale"][src*="pcc"]');
        if (pcc) {
            var versionmatch = pcc.src.match(/DeploymentConfigName=(.+)/i);
            if (versionmatch &amp;&amp; typeof ClickTaleExec === 'function') {
                ClickTaleExec("console.info('" + versionmatch[0] + "');");
                ClickTaleEvent("Config: " + versionmatch[1].replace(/\&amp;.+/, ''));
            }
        }
    });

    var ctRefferer = document.referrer.toLowerCase();
    var ctHref = document.referrer.toLowerCase();
    var _ctHostName = window.location.hostname.toLowerCase();
    var _ctPathName = window.location.pathname.toLowerCase();

    function isExistAndVisible(element) {
        if (typeof element === 'string' &amp;&amp; document.querySelector(element) !== null &amp;&amp; document.querySelector(element).offsetHeight &gt; 0 &amp;&amp; document.querySelector(element).offsetWidth &gt; 0) {
            return true;
        } else {
            return false;
        }
    }

    if (/\/registration\/index\/swid\/[^\/]+\//.test(window.location.pathname.toLowerCase()) &amp;&amp; (/(?:disneyworld.disney.go.com|wdw-stage.disney.go.com|wdw-latest.disney.go.com)/i.test(_ctHostName))) {
        var _ctPathName = "/registration/index/swid/"
    }

    function doUpload() { //core only
        if (typeof ClickTaleUploadPageNow === 'function' &amp;&amp; ClickTaleIsRecording()) {
            ClickTaleUploadPageNow();
        }
    };

    function checkGoodDOM() {
        if (/(?:disneyland.disney.go.com|dlr-stage.disney.go.com|dlr-latest.disney.go.com)/i.test(_ctHostName)) {
            if (_ctPathName.indexOf("/checkout") &gt; -1) {
                return isExistAndVisible('#pleaseWaitModal') !== true;
            }
        }
        else if (/(?:disneyworld.disney.go.com|wdw-stage.disney.go.com|wdw-latest.disney.go.com)/i.test(_ctHostName)) {
            if (_ctPathName.indexOf("/checkout/express/") &gt; -1 || _ctPathName.indexOf("/checkout/guests/") &gt; -1) {
                return (document.querySelector('div#pleaseWaitModal') !== null &amp;&amp; document.querySelector('div#pleaseWaitModal').offsetHeight == 0 &amp;&amp; document.querySelector('div#pleaseWaitModal').offsetWidth == 0);
            } else if (_ctPathName.indexOf('/registration/index/iscommerceflow/true/') &gt; -1 || _ctPathName.indexOf('/registration/index/swid/') &gt; -1) {
                return (document.querySelector('div#pleaseWaitModal') == null);
            } else if (_ctPathName.indexOf('/fastpass-plus/select/') &gt; -1) {
                return (document.querySelector('button.checkboxOption') !== null &amp;&amp; document.querySelector('button.checkboxOption').offsetHeight &gt; 0 &amp;&amp; document.querySelector('button.checkboxOption').offsetWidth &gt; 0);
            } else if ((_ctPathName === '' || _ctPathName === '/') &amp;&amp; !!document.querySelector('body.quickQuote')) {
                return !!document.querySelector('body.quickQuote.me-loaded') &amp;&amp; !!document.querySelector('#qqPanel.in');
            }
        }
        else if (/(?:disneycruise.disney.go.com|stage.disneycruise.disney.go.com)/i.test(_ctHostName)) {
            if (_ctPathName === '/') {
                return (document.querySelectorAll('.slidesHolder&gt;li.selected').length &gt; 0);
            }
            else if (_ctPathName.indexOf('/my-disney-cruise/reservations/') &gt; -1) {
                return (document.querySelectorAll('.itineraryItem').length &gt; 0 || document.querySelectorAll('[ng-bind-html="copy.section.labels.noItems"]').length &gt; 0);
            }
            else if (_ctPathName.indexOf('/guest-information/') &gt; -1) { //DISP-679
                return !!document.querySelector('#guestInformationForm .stateroom');
            }
            else {
                return !(document.querySelectorAll('wdpr-loading-spinner:not(.ng-hide)').length);
            }
        }
        return true;
    }

    function isReadyToRecord() {
        if (checkGoodDOM()) {
            //on recording start (in pcc) not all elements are yet in dom - waiting for delay upload to be done and then run again
            ClickTaleSettings.PTC.PCCinit = true;
            return (typeof ClickTaleUploadPageNow === 'function' &amp;&amp; ClickTaleIsRecording());
        }
        return false;
    }

    if (typeof ClickTaleDelayUploadPage === 'function') {
        ClickTaleDelayUploadPage();

        //AB Test Integration Timeout
        setTimeout(function () {
            window.ClickTaleSettings.PTC.doOnlyWhen(doUpload, isReadyToRecord, 400, 30, function () {
                ClickTaleSettings.PTC.PCCinit = true;
                doUpload();
            });
        }, 1000);

        var initFuncs = window.ClickTaleSettings.PTC.InitFuncs;
        for (var i = 0, initLen = initFuncs.length; i &lt; initLen; i++) {
            if (typeof initFuncs[i] === 'function') {
                initFuncs[i]();
            }
        }
    }


    //Cross Domain Tracking Start
    ClickTale(PID, Ratio, PartitionPrefix, SubsId);
    return;
    //Cross Domain Tracking End

    ClickTaleSettings.ForceRecording = function () {
        return (ctHref.indexOf('/dta-packages-std/') != -1 || ctRefferer.indexOf('disneytravelagents.com') != -1 || ctRefferer.indexOf('disneytravelagents.ca') != -1 || ctHref.indexOf('travel-agents') != -1);
    }
}

//Cross Domain Tracking Start
if (!window.ClickTaleSettings.PTC.StartRecordingNow) {
    function clickTaleCDWaitForID() {

        if (typeof clickTaleCDStartRecorder === "function") {
            if (window.ClickTaleSettings.PTC.StartRecordingNow) {
                return true;
            } else if (window.ClickTaleSettings.PTC.CrossDomainUID) {
                return true;
            }
        }
        return false;
    }
    window.ClickTaleSettings.PTC.doOnlyWhen(clickTaleCDStartRecorder, clickTaleCDWaitForID, 100, 100, clickTaleCDStartRecorder);
} else {
    clickTaleCDStartRecorder();
}
//Cross Domain Tracking End

	// End of user-defined header code (PreInitialize)
    
	
	window.ClickTaleIncludedOnDOMReady=true;
	
	ClickTaleGlobal.init.isAllowed &amp;&amp; ClickTale(PID, Ratio, PartitionPrefix, SubsId);
	
	if((typeof ClickTalePrevOnReady == 'function') &amp;&amp; (ClickTaleOnReady.toString() != ClickTalePrevOnReady.toString()))
	{
    	ClickTalePrevOnReady();
	}
	
	
	// Start of user-defined footer code
	
	// End of user-defined footer code
	
}; 
(function() {
	var div = ClickTaleCreateDOMElement("div");
	div.id = "ClickTaleDiv";
	div.style.display = "none";
	document.body.appendChild(div);

	
	
	var wrScript = ClickTaleCreateDOMElement("script");
	wrScript.crossOrigin = "anonymous";	
	wrScript.src = window.ClickTaleScriptSource + 'WR1012.js';
	wrScript.type = 'text/javascript';
		wrScript.async = true;
		ClickTaleGlobal.init.isAllowed &amp;&amp; document.body.appendChild(wrScript);
})();





