        search: /(?:disneyworld.disney.go.com|wdw-stage.disney.go.com|wdw-latest.disney.go.com)/i
    }
}, {
    selector: ".travelGuest .rawOption,.travelGuest option,.guestDetailLabel,.contactEmail,.contactPhone,#friendList option,.friendList option,.magicConfirmContainer .confirmHeader,[aria-owns*='addGuest'] .rawOption,#addGuest1-guestSelectBox option,.guestSelectBox option,.friendListContainer .listWrapper .rawOption,[ng-repeat='paymentItem in paymentsApplied'] .leftSide",
    changeMon: {
        Attributes: false,
        Text: true
    },
    rewriteApi: {
        Attributes: false,
        Text: true
    },
    location: {
        prop: 'href',
        search: /(?:disneyworld.disney.go.com|wdw-stage.disney.go.com|wdw-latest.disney.go.com)/i
    }
}, {
    selector: ".travelPartyItem .media-body div:not(.extra-small-copy),#monthlyPaymentAgreementModalContainer td,#monthlyPaymentAgreementModalContainer td span",
    changeMon: {
        Attributes: false,
        Text: true
    },
    rewriteApi: {
        Attributes: false,
        Text: true
    },
    location: {
        prop: 'href',
        search: /(?:disneyworld.disney.go.com|wdw-stage.disney.go.com|wdw-latest.disney.go.com)/i
    }
}, {
    selector: ".confirm .body-copy.subtitle,.insurance-guest-selection label span",
    changeMon: {
        Attributes: false,
        Text: true
    },
    rewriteApi: {
        Attributes: false,
        Text: true
    },
    location: {
        prop: 'href',
        search: /(?:disneycruise.disney.go.com|stage.disneycruise.disney.go.com)/i
    }
}, {
    selector: ".friendSelect *",
    changeMon: {
        Attributes: false,
        Text: true
    },
    rewriteApi: {
        Attributes: false,
        Text: true
    },
    location: {
        prop: 'href',
        search: /disneyworld\.disney\.go\.com\/checkout\/express\/$/i
    }
}, {
    /* class guestSensitive will do it :
    1 character will be fully masked
    2 characters will expose just the first character
    3 or more characters will expose just the first 2 characters*/
    selector: ".guestSensitive, .guestSensitiveHTMLtag-strong strong, .guestSensitiveHTMLtag-b b, .guestSensitiveHTMLtag-strong ctdep-strong, .guestSensitiveHTMLtag-b ctdep-b",
    changeMon: {
        Attributes: ['value'],
        Text: true
    },
    rewriteApi: {
        Attributes: ['value'],
        Text: true
    }
}, {
    //guestSensitiveHigh  class will mask the whole string
    selector: ".guestSensitiveHigh",
    changeMon: {
        Attributes: ['value'],
        Text: true
    },
    rewriteApi: {
        Attributes: ['value'],
        Text: true
    }
}, {
    //guestSensitiveNot  class will unmask the whole string
    selector: ".guestSensitiveNot",
    changeMon: {
        Attributes: ['value'],
        Text: true
    },
    rewriteApi: {
        Attributes: ['value'],
        Text: true
    }
